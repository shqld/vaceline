{
  "name": "vaceline",
  "version": "0.0.6",
  "main": "dist/index.js",
  "repository": "git@github.com:shqld/vaceline.git",
  "author": "Sho Miyamoto <me@shomiyamoto.dev>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "vaceline": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "npm run -s build > /dev/null && node dist/cli",
    "build": "rimraf dist && npm run -s generate && babel src -d dist -x .ts,.js",
    "generate": "node tasks/generate",
    "inspect": "yarn lint && yarn typecheck && yarn test",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "bench": "node benchmark > bench.txt",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@textlint/markdown-to-ast": "^6.1.6",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "is-ip": "^3.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.2.0",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.8",
    "@types/prettier": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "benchmark": "^2.1.4",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "ts-morph": "^6.0.2",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn pretty-quick && yarn generate && yarn inspect"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run -s typecheck",
      "npm run -s lint"
    ]
  }
}
